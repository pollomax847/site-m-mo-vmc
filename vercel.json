{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      }
    }
  ],
  "github": {
    "enabled": true,
    "silent": false
  },
  "buildCommand": "echo 'Build command executed'",
  "cleanUrls": true
}
